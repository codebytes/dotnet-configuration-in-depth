{
  "MarkdownConverter": {
    "markdownStyle": "github",
    "pdf": {
      "FileExtension": "pdf",
      "OutputDir": "./output/pdf/",
      "TemplateFile": "./templates/template.pdf"
    },
    "doc": {
      "FileExtension": "doc",
      "OutputDir": "./output/doc/",
      "TemplateFile": "./templates/template.doc"
    },
    "html": {
      "FileExtension": "html",
      "OutputDir": "./output/html/",
      "TemplateFile": "./templates/template.html"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}